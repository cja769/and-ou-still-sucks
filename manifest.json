{
  "name": "OU-to-OU SUCKS",
  "manifest_version": 2,
  "version": "1.1",
  "description": "Replaces all references to OU (University of Oklahoma) with OU SUCKS",
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["contentscript.js"]
    }
  ],
  "icons" : {"16" : "longhorn16.png", "48" : "OU_SUCKS48.png", "128" : "longhorn128.png"}
}